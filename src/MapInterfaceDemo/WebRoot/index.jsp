<%@ page language="java" import="java.util.*" pageEncoding="utf-8"%><%String path = request.getContextPath();String basePath = request.getScheme()+"://"+request.getServerName()+":"+request.getServerPort()+path+"/";%><!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"><html>  <head>    <base href="<%=basePath%>">        <title>My JSP 'index.jsp' starting page</title>	<meta http-equiv="pragma" content="no-cache">	<meta http-equiv="cache-control" content="no-cache">	<meta http-equiv="expires" content="0">    	<meta http-equiv="keywords" content="keyword1,keyword2,keyword3">	<meta http-equiv="description" content="This is my page">	<script src="http://code.jquery.com/jquery-1.11.1.min.js"></script>  </head>  <body>  	<div>	<ul>	<li><label>服务器</label>	<select name="host" id="host">	<option value="http://localhost:8080/MapInterfaceDemo/map/demo">http://121.199.3.89:4080</option>	<option value="http://121.199.3.89:4081/map/demo/">http://121.199.3.89:4081/map/demo/</option>	</select>	<li><label>接口名</label>	<select name="api" id="api">	<option value="/gw/userAuth/get_smscode">/gw/userAuth/get_smscode</option>	<option value="/gw/userAuth/login">/gw/userAuth/login</option>	<option value="/gw/cashAccount/pay_yyd">/gw/cashAccount/pay_yyd</option>	<option value="/gw/investmentAccount/savings_by_accountant">/gw/investmentAccount/savings_by_accountant</option>	<option value="/gw/investmentAccount/withdraw_by_accountant">/gw/investmentAccount/withdraw_by_accountant</option>	<option value="/gw/investmentAccount/transfer_to_bankCard">/gw/investmentAccount/transfer_to_bankCard</option>	<option value="/gw/investmentAccount/transfer_to_cashAccount">/gw/investmentAccount/transfer_to_cashAccount</option>	<option value="/gw/investmentAccount/pay_yyd">/gw/investmentAccount/pay_yyd</option>	<option value="/gw/investmentAccount/payback_capital">/gw/investmentAccount/payback_capital</option>	<option value="/gw/investmentAccount/payback_interest">/gw/investmentAccount/payback_interest</option>	<option value="/gw/investmentAccount/transfer_to_bankCard_undo">/gw/investmentAccount/transfer_to_bankCard_undo</option>	<option value="/gw/bankCard/pay_yyd">/gw/bankCard/pay_yyd</option>	<option value="/gw/huitongCard/transfer_to_cashAccount">/gw/huitongCard/transfer_to_cashAccount</option>	</select>	</li>	<li><label>参数</label><textarea id="param" cols="100" rows="20"></textarea></li>	<li><input type="button" value="提交" id="bt"></li>	<li><label>响应</label><div id="resp"></div></li>	</ul>	</div>  </body>    <script>		$("#bt").click(function (){			var url =$("#host").val()+$("#api").val();			var data = $("#param").val();			$.ajax({type:'POST',contentType:'application/json',url:url,data:data,			success:function(resp){ $("#resp").html(resp);},dataType:"html"}			);			});								</script>  </html>